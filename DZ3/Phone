class Phone {

int number;
 String model;
  double weight;

 void Main (String [] args) {
     Phone phone1 = new Phone();
     phone1.number = 123456789;
     phone1.model = "Iphone";
     phone1.weight = 185.5;

     System.out.println("Номер телефона: " + number);
     System.out.println("Модель телефона: " + model);
     System.out.println("Вес телефона: " + weight);
 }
}
